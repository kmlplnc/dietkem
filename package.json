{
  "name": "dietkem",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force -ErrorAction SilentlyContinue node_modules, apps/*/node_modules, packages/*/node_modules, apps/*/dist, apps/*/.next, apps/*/.output, .turbo\"",
    "postinstall": "turbo run build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "drizzle-orm": "^0.29.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.8",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "turbo": "^1.12.4"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
