{
  "name": "@dietkem/db",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsup",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "tsx src/migrate.ts",
    "db:manual-migrate": "tsx src/manual_migrate.ts",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "drizzle-orm": "^0.29.3",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.20.14",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
